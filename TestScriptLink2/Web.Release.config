<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <connectionStrings>
        <!--Cache Connections-->
        <add name="AvatarDBPM" connectionString="DRIVER={InterSystems ODBC};SERVER=192.1.5.216;PORT=1972;DATABASE=HMHCPMLIVE;UID=LIVE:ODBC;PWD=hotwire2011;" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
        <add name="AvatarDBCWS" connectionString="DRIVER={InterSystems ODBC};SERVER=192.1.5.216;PORT=1972;DATABASE=HMHCPMLIVE;UID=LIVE:ODBC;PWD=hotwire2011;" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
